{
  "global": {
    "color": {
      "sidebar": {
        "start": {
          "$type": "color",
          "$value": "#1a1d24"
        },
        "end": {
          "$type": "color",
          "$value": "#13161a"
        }
      },
      "sidebar-text": {
        "$type": "color",
        "$value": "#e4e6eb"
      },
      "sidebar-border": {
        "$type": "color",
        "$value": "rgba(255, 255, 255, 0.06)"
      },
      "nav-default": {
        "$type": "color",
        "$value": "#9ca3af"
      },
      "nav-hover-bg": {
        "$type": "color",
        "$value": "rgba(255, 255, 255, 0.06)"
      },
      "nav-active-bg": {
        "$type": "color",
        "$value": "rgba(251, 191, 36, 0.15)"
      },
      "accent": {
        "$type": "color",
        "$value": "#fbbf24"
      },
      "accent-dark": {
        "$type": "color",
        "$value": "#f59e0b"
      },
      "avatar-text": {
        "$type": "color",
        "$value": "#13161a"
      },
      "muted": {
        "$type": "color",
        "$value": "#6b7280"
      },
      "main-bg": {
        "$type": "color",
        "$value": "#f5f5f4"
      },
      "heading": {
        "$type": "color",
        "$value": "#f00"
      },
      "card-bg": {
        "$type": "color",
        "$value": "#ffffff"
      },
      "success": {
        "$type": "color",
        "$value": "#059669"
      },
      "error": {
        "$type": "color",
        "$value": "#dc2626"
      },
      "border": {
        "$type": "color",
        "$value": "#f3f4f6"
      },
      "chart-label": {
        "$type": "color",
        "$value": "#9ca3af"
      }
    },
    "font": {
      "family": {
        "$type": "fontFamily",
        "$value": [
          "Plus Jakarta Sans",
          "system-ui",
          "-apple-system",
          "sans-serif"
        ]
      },
      "size": {
        "xs": {
          "$type": "dimension",
          "$value": "0.75rem"
        },
        "sm": {
          "$type": "dimension",
          "$value": "0.8125rem"
        },
        "base": {
          "$type": "dimension",
          "$value": "0.9375rem"
        },
        "md": {
          "$type": "dimension",
          "$value": "1rem"
        },
        "lg": {
          "$type": "dimension",
          "$value": "1.25rem"
        },
        "xl": {
          "$type": "dimension",
          "$value": "1.5rem"
        },
        "2xl": {
          "$type": "dimension",
          "$value": "1.75rem"
        }
      },
      "weight": {
        "medium": {
          "$type": "fontWeight",
          "$value": "500"
        },
        "semibold": {
          "$type": "fontWeight",
          "$value": "600"
        }
      },
      "letter-spacing": {
        "$type": "dimension",
        "$value": "-0.02em"
      }
    },
    "spacing": {
      "xs": {
        "$type": "dimension",
        "$value": "0.125rem"
      },
      "sm": {
        "$type": "dimension",
        "$value": "0.25rem"
      },
      "md": {
        "$type": "dimension",
        "$value": "0.5rem"
      },
      "lg": {
        "$type": "dimension",
        "$value": "0.75rem"
      },
      "xl": {
        "$type": "dimension",
        "$value": "1rem"
      },
      "2xl": {
        "$type": "dimension",
        "$value": "1.25rem"
      },
      "3xl": {
        "$type": "dimension",
        "$value": "1.5rem"
      },
      "4xl": {
        "$type": "dimension",
        "$value": "2rem"
      }
    },
    "radius": {
      "sm": {
        "$type": "dimension",
        "$value": "6px"
      },
      "md": {
        "$type": "dimension",
        "$value": "10px"
      },
      "lg": {
        "$type": "dimension",
        "$value": "12px"
      }
    },
    "layout": {
      "sidebar-width": {
        "$type": "dimension",
        "$value": "260px"
      },
      "avatar-size": {
        "$type": "dimension",
        "$value": "40px"
      },
      "chart-height": {
        "$type": "dimension",
        "$value": "180px"
      },
      "chart-bar-min": {
        "$type": "dimension",
        "$value": "24px"
      }
    },
    "shadow": {
      "card": {
        "$type": "shadow",
        "$value": "0 1px 3px rgba(0, 0, 0, 0.06)"
      }
    },
    "transition": {
      "fast": {
        "$type": "duration",
        "$value": "0.2s"
      },
      "chart": {
        "$type": "duration",
        "$value": "0.3s"
      }
    },
    "breakpoint": {
      "tablet": {
        "$type": "dimension",
        "$value": "1024px"
      },
      "mobile": {
        "$type": "dimension",
        "$value": "768px"
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "global"
    ]
  }
}